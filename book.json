{
  "author": "Bai Yingjie",
  "language": "zh-hans",
  "plugins": [
    "-sharing",
    "-lunr",
    "-search",
    "-highlight",
    "search-plus",
    "github",
    "disqus",
    "prism",
    "anchors",
    "splitter",
    "wide-page",
    "expandable-chapters-small2",
    "anchor-navigation-ex"
  ],

  "pluginsConfig": {
    "github": {
      "url": "https://github.com/Bai-Yingjie/Bai-Yingjie.github.io"
    },
    "disqus": {
      "shortName": "bai-yingjie-notes"
    },
    "pluginsConfig": {
      "prism": {
        "css": [
          "prism-themes/themes/prism-coldark-cold.css"
        ]
      }
    },
    "expandable-chapters-small2":{
      "articlesExpand": true
    },
    "anchor-navigation-ex": {
      "isRewritePageTitle": true,
      "isShowTocTitleIcon": true,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    }
  }
}
