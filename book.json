{
  "author": "Bai Yingjie",
  "title": "My Notes",
  "language": "zh-hans",
  "plugins": [
    "-livereload",
    "-sharing",
    "-lunr",
    "-search",
    "search-plus",
    "-highlight",
    "theme-comscore",
    "prism",
    "code",
    "chapter-fold",
    "github",
    "splitter",
    "wide-page",
    "hide-navigation-buttons",
    "anchor-navigation-ex",
    "sequence@0.1.1",
    "mermaid-gb3"
  ],

  "pluginsConfig": {
    "github": {
      "url": "https://github.com/Bai-Yingjie/Bai-Yingjie.github.io"
    },
    "prism": {
      "css": [
        "prismjs/themes/prism.css"
      ],
      "ignore": [
        "mermaid",
        "sequence"
      ]
    },
    "anchor-navigation-ex": {
      "isRewritePageTitle": true,
      "isShowTocTitleIcon": true,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    }
  }
}
